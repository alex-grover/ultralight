{
  "name": "ultralight",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.2.3",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "format": "prettier --cache --ignore-unknown",
    "check:format": "bun run format --check .",
    "fix:format": "bun run format --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache",
    "check:lint": "bun run lint .",
    "fix:lint": "bun run check:lint --fix",
    "styles": "stylelint --cache --cache-location node_modules/.cache",
    "check:styles": "bun run styles ./**/*.css",
    "fix:styles": "bun run check:styles --fix",
    "check:types": "tsc"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.5.5"
  },
  "devDependencies": {
    "@alex-grover/eslint-config": "^0.0.10",
    "@alex-grover/lefthook-config": "^0.0.5",
    "@alex-grover/prettier-config": "^0.0.6",
    "@alex-grover/stylelint-config": "^0.0.6",
    "@eslint/eslintrc": "^3.3.1",
    "@tsconfig/next": "^2.0.3",
    "@tsconfig/strictest": "^2.0.6",
    "@types/node": "^20.19.21",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.32.0",
    "lefthook": "^1.13.6",
    "prettier": "3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-gamut": "^1.3.4",
    "stylelint-plugin-use-baseline": "^1.0.2",
    "typescript": "^5.9.3"
  },
  "prettier": "@alex-grover/prettier-config",
  "stylelint": {
    "extends": "@alex-grover/stylelint-config"
  }
}
